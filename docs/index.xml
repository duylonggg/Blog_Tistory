  <variables>
    <variablegroup name="Sidebar">
      <variable>
        <name>sidebar</name>
        <label>Expand/Collapse Sidebar</label>
        <description><![CDATA[ Sidebar ]]></description>
        <type>BOOL</type>
        <default>true</default>
      </variable>
      <variable>
        <name>foldable-category</name>
        <label>Collapse/Expand Categories</label>
        <description><![CDATA[ Collapse/Expand Categories ]]></description>
        <type>BOOL</type>
        <default>true</default>
      </variable>
    </variablegroup>
    <variablegroup name="Post">
      <variable>
        <name>header-style</name>
        <label>Header Style</label>
        <description><![CDATA[ Header Style ]]></description>
        <type>SELECT</type>
        <option><![CDATA[
          [
            {"name":"simple", "label":"Simple", "value":"simple"},
            {"name":"thumbnail", "label":"Thumbnail", "value":"thumbnail"},
            {"name":"screen", "label":"Screen", "value":"screen"}
          ]
        ]]></option>
        <default>thumbnail</default>
      </variable>
      <variable>
        <name>width</name>
        <label>Width</label>
        <description><![CDATA[ Width ]]></description>
        <type>STRING</type>
        <default>720</default>
      </variable>
      <variable>
        <name>toc</name>
        <label>Table Of Contents</label>
        <description><![CDATA[ Table Of Contents ]]></description>
        <type>BOOL</type>
        <default>true</default>
      </variable>
      <variable>
        <name>scrollspy</name>
        <label>Table Of Contents (Scrollspy)</label>
        <description><![CDATA[ Table Of Contents (Scrollspy) ]]></description>
        <type>BOOL</type>
        <default>true</default>
      </variable>
      <variable>
        <name>hljs</name>
        <label>Code Highlighting (Light)</label>
        <description><![CDATA[ Code Highlighting (Light) ]]></description>
        <type>STRING</type>
        <default>xcode</default>
      </variable>
      <variable>
        <name>hljs-dark</name>
        <label>Code Highlighting (Dark)</label>
        <description><![CDATA[ Code Highlighting (Dark) ]]></description>
        <type>STRING</type>
        <default>vs2015</default>
      </variable>
    </variablegroup>
  </variables>
  <default>
    <liststyle>list</liststyle>
    <recentEntries>5</recentEntries>
    <recentComments>5</recentComments>
    <lengthOfRecentNotice>32</lengthOfRecentNotice>
    <lengthOfRecentEntry>32</lengthOfRecentEntry>
    <lengthOfRecentComment>32</lengthOfRecentComment>
  </default>
